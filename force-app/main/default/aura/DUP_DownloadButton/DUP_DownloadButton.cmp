<aura:component description="download button" controller="DUP_DownloadButtonController">
    <aura:attribute name="isSuperUser" type="Boolean" access="public"/>
	<aura:attribute name="fileId" type="String" access="public"/>  
    
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    
    <aura:if isTrue="{!v.isSuperUser}">
        <lightning:button label="Download" onclick="{!c.downloadDocument}"/>
    </aura:if>
    
</aura:component>