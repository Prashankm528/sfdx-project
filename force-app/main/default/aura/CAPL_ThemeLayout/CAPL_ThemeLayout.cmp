<aura:component implements="forceCommunity:themeLayout" access="global" description="COM Theme Layout">
    <aura:attribute name="search" type="Aura.Component[]" required="false"/>
    <aura:attribute name="profileMenu" type="Aura.Component[]" required="false"/>
    <aura:attribute name="navBar" type="Aura.Component[]" required="false"/>
    <aura:attribute name="newHeader" type="Aura.Component[]" required="false"/>
     <aura:attribute name="newFooter" type="Aura.Component[]" required="false"/>

    <div class="navHeader"> 
		<div class="navMenuHeader slds-grid slds-wrap slds-p-vertical_medium">
	        <div class="bpLogo slds-col slds-size_12-of-12 slds-medium-size_3-of-12 slds-large-size_3-of-12">
	            <img class="castrolLogo" onclick="{!c.goHome}" src="{!$Resource.CAPL_Resources + '/CAPL_Resources/images/CastrolLogo.png'}" />
	        </div>
	        
	        <div class="showOnMobile slds-col slds-size_7-of-12 slds-medium-size_0-of-12 slds-large-size_0-of-12" />
	        
	        <div class="searchComponent slds-col slds-size_2-of-12 slds-medium-size_6-of-12 slds-large-size_6-of-12">
	        	<div class="slds-grid slds-grid_align-center">
	                <div class="slds-col slds-size_1-of-1 slds-p-horizontal_medium">
		               {!v.search}
	                </div>
		        </div>
	        </div>
	        <div class="notificationProfilePanel slds-col slds-size_3-of-12 slds-small-size_3-of-12 slds-medium-size_3-of-12 slds-large-size_3-of-12">
	        	<div class="rightPanel slds-grid slds-grid_align-end">
		        	<div class="notifications">
			            <forceCommunity:notifications />
			        </div>
		            <div class="profileMenuRegion">
			            {!v.profileMenu}
			        </div>
			    </div>
	        </div>
	    </div>
	    <div class="navigation">
            {!v.navBar}
        </div>

	</div>

    <div class="newHeader slds-grid_align-center">
        {!v.newHeader}
    </div>
    <div class="mainContentArea slds-container_x-large slds-container_center">
        <div class="slds-col">
            {!v.body}
        </div>
    </div>
    <div class="newFooter">
        {!v.newFooter}
    </div>
</aura:component>